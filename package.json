{
    "name": "shop.astro.build",
    "type": "module",
    "version": "0.0.1",
    "license": "MIT",
    "engines": {
        "node": "20.17.0"
    },
    "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c",
    "scripts": {
        "dev:ci": "doppler run pnpm openapi-ts && doppler run astro dev",
        "dev": "astro dev",
        "netdev": "astro dev --host",
        "start": "astro dev",
        "build:ci": "pnpm openapi-ts && pnpm astro build",
        "build": "pnpm astro build",
        "astro": "astro",
        "lint": "astro sync && run-s --continue-on-error lint:*",
        "lint:biome": "biome lint src",
        "lint:astro": "astro check",
        "lint:types": "tsc -b",
        "test:e2e": "playwright test",
        "test:unit": "vitest",
        "format": "run-s --continue-on-error format:*",
        "format:biome": "biome check . --apply",
        "format:prettier": "prettier . --cache --write --list-different"
    },
    "devDependencies": {
        "@astrojs/check": "0.7.0",
        "@biomejs/biome": "^1.4.1",
        "@hey-api/openapi-ts": "^0.53.0",
        "@types/node": "^20.10.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.1.1",
        "prettier-plugin-astro": "^0.12.2",
        "prettier-plugin-jsdoc": "^1.1.1",
        "prettier-plugin-tailwindcss": "^0.5.9",
        "typescript": "^5.3.3",
        "vite": "^5.4.5",
        "vitest": "^2.1.1"
    },
    "dependencies": {
        "@astrojs/solid-js": "^4.4.2",
        "@astrojs/tailwind": "^5.1.1",
        "@astrojs/vercel": "^7.8.1",
        "@fontsource-variable/inter": "^5.1.0",
        "@hey-api/client-fetch": "^0.4.0",
        "@kobalte/core": "^0.13.7",
        "@kobalte/tailwindcss": "^0.9.0",
        "@kobalte/utils": "^0.9.1",
        "@playwright/test": "^1.48.0",
        "@sentry/astro": "^8.33.1",
        "@solid-primitives/upload": "^0.0.117",
        "@tailwindcss/typography": "^0.5.15",
        "@tanstack/solid-query": "^5.59.6",
        "@tanstack/solid-query-devtools": "^5.59.6",
        "astro": "^4.15.12",
        "astro-icon": "^1.1.1",
        "firebase": "^10.14.0",
        "firebase-admin": "^12.6.0",
        "loops": "^3.3.0",
        "solid-icons": "^1.1.0",
        "solid-js": "^1.9.2",
        "stripe": "^17.1.0",
        "tailwind-merge": "^2.5.3",
        "tailwindcss": "^3.4.13",
        "tailwindcss-animate": "^1.0.7",
        "type-fest": "^4.26.1",
        "zod": "^3.23.8"
    }
